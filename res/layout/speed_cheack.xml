<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back1" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:text="スピード調整"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#ff8f77"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView2"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:text="ポケモン名"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/textView"
        android:layout_below="@+id/textView"
        android:layout_marginTop="10dp"
        android:text="せいかく"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/level1"
        android:layout_marginBottom="2dp"
        android:layout_toLeftOf="@+id/level1"
        android:text="レベル"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#ffffff" />

    <AutoCompleteTextView
        android:id="@+id/name1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_marginBottom="2dp"
        android:layout_toLeftOf="@+id/TextView04"
        android:layout_toRightOf="@+id/textView3"
        android:background="@drawable/waku"
        android:hint="ポケモン名"
        android:inputType="text" />

    <Spinner
        android:id="@+id/seikaku1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView3"
        android:layout_below="@+id/textView"
        android:layout_marginTop="5dp"
        android:layout_toLeftOf="@+id/TextView03"
        android:layout_toRightOf="@+id/textView"
        android:background="@drawable/waku"
        android:entries="@array/seikaku"
        android:text="0" />

    <EditText
        android:id="@+id/level1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/textView1"
        android:layout_below="@+id/seikaku1"
        android:layout_marginTop="4dp"
        android:layout_toRightOf="@+id/textView3"
        android:background="@drawable/waku"
        android:inputType="text"
        android:layout_marginBottom="3dp"
        android:text="50" />

    <EditText
        android:id="@+id/kotaiti1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/level1"
        android:layout_alignRight="@+id/textView1"
        android:layout_below="@+id/level1"
        android:background="@drawable/waku"
        android:ems="10"
        android:inputType="text"
        android:layout_marginBottom="3dp"
        android:text="31" />

    <TextView
        android:id="@+id/TextView01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/kotaiti1"
        android:layout_toLeftOf="@+id/kotaiti1"
        android:text="個体値"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#ffffff" />

    <EditText
        android:id="@+id/doryokuti1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/kotaiti1"
        android:layout_alignRight="@+id/textView1"
        android:layout_below="@+id/kotaiti1"
        android:background="@drawable/waku"
        android:ems="10"
        android:inputType="text"
        android:text="252" />

    <TextView
        android:id="@+id/TextView02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/doryokuti1"
        android:layout_alignBottom="@+id/doryokuti1"
        android:layout_toLeftOf="@+id/doryokuti1"
        android:text="努力値"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/TextView03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="道具"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#ffffff"
        android:layout_above="@+id/textView4"
        android:layout_alignLeft="@+id/TextView04" />

    <TextView
        android:id="@+id/TextView04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="特性"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#ffffff"
        android:layout_above="@+id/textView3"
        android:layout_centerHorizontal="true" />

    <Spinner
        android:id="@+id/tokusei_Spinner1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/dougu_Spinner1"
        android:layout_alignRight="@+id/oikaze_checkBox1"
        android:layout_below="@+id/textView2"
        android:layout_toRightOf="@+id/TextView04"
        android:background="@drawable/waku"
        android:entries="@array/tokusei1"
        android:layout_marginBottom="3dp"
        android:text="0" />

    <Spinner
        android:id="@+id/dougu_Spinner1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView4"
        android:layout_alignLeft="@+id/tokusei_Spinner1"
        android:layout_alignRight="@+id/tokusei_Spinner1"
        android:layout_alignTop="@+id/textView3"
        android:background="@drawable/waku"
        android:entries="@array/dougu1"
        android:text="0" />

    <CheckBox
        android:id="@+id/mahi_checkBox1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/oikaze_checkBox1"
        android:layout_alignBottom="@+id/oikaze_checkBox1"
        android:layout_alignLeft="@+id/TextView03"
        android:text="まひ"
        android:textColor="#ffffff" />

    <CheckBox
        android:id="@+id/oikaze_checkBox1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Rank_Spinner1"
        android:layout_toRightOf="@+id/mahi_checkBox1"
        android:text="おいかぜ"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="レベル"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#ffffff"
        android:layout_below="@+id/textView5"
        android:layout_alignRight="@+id/textView5"
        android:layout_marginTop="10dp" />

    <AutoCompleteTextView
        android:id="@+id/name2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/level2"
        android:layout_below="@+id/textView8"
        android:layout_marginBottom="3dp"
        android:layout_toLeftOf="@+id/textView11"
        android:background="@drawable/waku"
        android:hint="ポケモン名"
        android:inputType="text" />

    <Spinner
        android:id="@+id/seikaku_spinner2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/level2"
        android:layout_alignLeft="@+id/name2"
        android:layout_alignRight="@+id/name2"
        android:layout_below="@+id/name2"
        android:layout_marginBottom="3dp"
        android:background="@drawable/waku"
        android:entries="@array/seikaku"
        android:text="0" />

    <EditText
        android:id="@+id/level2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/doryokuti1"
        android:layout_alignTop="@+id/textView6"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@+id/TextView02"
        android:background="@drawable/waku"
        android:inputType="text"
        android:text="50" />

    <EditText
        android:id="@+id/kotaiti2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/waku"
        android:ems="10"
        android:inputType="text"
        android:text="31"
        android:layout_below="@+id/level2"
        android:layout_alignLeft="@+id/level2"
        android:layout_marginTop="3dp"
        android:layout_marginBottom="3dp"
        android:layout_alignRight="@+id/doryokuti1" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="個体値"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#ffffff"
        android:layout_below="@+id/level2"
        android:layout_alignLeft="@+id/textView6" />

    <EditText
        android:id="@+id/doryokuti2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/waku"
        android:ems="10"
        android:inputType="text"
        android:text="252"
        android:layout_below="@+id/kotaiti2"
        android:layout_alignLeft="@+id/kotaiti2"
        android:layout_alignRight="@+id/doryokuti1" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="努力値"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#ffffff"
        android:layout_below="@+id/kotaiti2"
        android:layout_alignLeft="@+id/textView7" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="特性"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#ffffff"
        android:layout_alignBottom="@+id/name2"
        android:layout_alignLeft="@+id/mahi_checkBox1" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ポケモン名"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#ffffff"
        android:layout_below="@+id/textView8"
        android:layout_alignParentLeft="true" />

    <Spinner
        android:id="@+id/tokusei2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/seikaku_spinner2"
        android:layout_alignRight="@+id/dougu_Spinner2"
        android:layout_alignTop="@+id/name2"
        android:layout_toRightOf="@+id/textView11"
        android:background="@drawable/waku"
        android:entries="@array/tokusei1"
        android:text="0" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView14"
        android:text="実測値"
        android:textColor="#ffffff"/>

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button1"
        android:layout_toRightOf="@+id/textView16"
        android:text="比較元ポケモン"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView16"
        android:layout_toRightOf="@+id/textView2"
        android:text="比較先ポケモン"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#ffffff" />

    <EditText
        android:id="@+id/kekka_SPE1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TextView02"
        android:layout_below="@+id/textView14"
        android:layout_toLeftOf="@+id/textView10"
        android:background="@drawable/waku"
        android:ems="10" >

        <requestFocus />
    </EditText>

    <EditText
        android:id="@+id/kekka_SPE2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/textView15"
        android:layout_below="@+id/textView15"
        android:layout_toRightOf="@+id/textView2"
        android:background="@drawable/waku"
        android:ems="10" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/TextView06"
        android:text="スピード調整にお使いください"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/TextView06"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView1"
        android:text="比較元のポケモン"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#4cfffa" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView"
        android:layout_below="@+id/mahi_checkBox1"
        android:text="比較先のポケモン"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#4cfffa" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/textView13"
        android:layout_below="@+id/name2"
        android:text="せいかく"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/seikaku_spinner2"
        android:layout_toLeftOf="@+id/Rank_Spinner1"
        android:text="道具"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#ffffff" />

    <Spinner
        android:id="@+id/dougu_Spinner2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/level2"
        android:layout_alignLeft="@+id/Rank_Spinner2"
        android:layout_alignRight="@+id/Rank_Spinner2"
        android:layout_alignTop="@+id/seikaku_spinner2"
        android:layout_marginTop="3dp"
        android:background="@drawable/waku"
        android:entries="@array/dougu1"
        android:text="0" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/level2"
        android:layout_alignBottom="@+id/level2"
        android:layout_alignRight="@+id/textView10"
        android:text="ランク"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#ffffff" />

    <Spinner
        android:id="@+id/Rank_Spinner2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/kotaiti2"
        android:layout_alignLeft="@+id/Rank_Spinner1"
        android:layout_alignRight="@+id/oikaze_checkBox2"
        android:layout_alignTop="@+id/level2"
        android:background="@drawable/waku"
        android:entries="@array/rank"
        android:text="0" />

    <CheckBox
        android:id="@+id/mahi_checkBox2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView9"
        android:layout_alignBottom="@+id/textView9"
        android:layout_alignLeft="@+id/textView12"
        android:text="まひ"
        android:textColor="#ffffff" />

    <CheckBox
        android:id="@+id/oikaze_checkBox2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/mahi_checkBox2"
        android:layout_alignBottom="@+id/mahi_checkBox2"
        android:layout_alignLeft="@+id/oikaze_checkBox1"
        android:text="おいかぜ"
        android:textColor="#ffffff" />

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/mahi_checkBox2"
        android:layout_marginTop="17dp"
        android:text="算出"
        android:textColor="#FFFFFF" />

    <TextView
        android:id="@+id/TextView05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/TextView01"
        android:layout_toLeftOf="@+id/Rank_Spinner1"
        android:text="ランク"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#ffffff" />

    <Spinner
        android:id="@+id/Rank_Spinner1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/kotaiti1"
        android:layout_alignRight="@+id/oikaze_checkBox1"
        android:layout_alignTop="@+id/TextView05"
        android:layout_toRightOf="@+id/TextView03"
        android:background="@drawable/waku"
        android:entries="@array/rank"
        android:layout_marginBottom="5dp"
        android:text="0" />

</RelativeLayout>